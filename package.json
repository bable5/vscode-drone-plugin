{
  "name": "mooneyware-drone-viewer",
  "displayName": "mooneyware-drone-viewer",
  "description": "Helps manage and understand large drone pipeline files",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.74.0"
  },
  "author": {
    "name": "Sean L. Mooney"
  },
  "publisher": "MooneyWare",
  "license": "MIT",
  "repository": {
    "url": "https://gitlab.com/bable5/drone-extension"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:yaml"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [],
    "viewsContainers": {
			"activitybar": [
				{
					"id": "drone-pipeline-explorer",
					"title": "Drone Pipeline Explorer",
					"icon": "media/dep.svg"
				}
			]
		},
		"views": {
			"drone-pipeline-explorer": [
				{
					"id": "drone-viewer.droneDependencies",
					"name": "Drone Pipeline Explorer",
					"icon": "media/dep.svg",
					"contextualTitle": "Drone Pipeline Explorer"
				}
			]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "vscode:package": "vsce package",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vscode/test-electron": "^2.2.2",
    "@vscode/vsce": "^2.17.0",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4"
  }
}
